{
  "name": "@safe-global/safe-cli",
  "version": "0.1.0",
  "description": "Modern CLI for Safe Smart Account management",
  "type": "module",
  "bin": {
    "safe": "./dist/index.js"
  },
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsup",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "safe",
    "gnosis-safe",
    "multisig",
    "ethereum",
    "cli",
    "smart-account",
    "account-abstraction"
  ],
  "author": "Safe Global",
  "license": "MIT",
  "dependencies": {
    "@clack/prompts": "^0.7.0",
    "@safe-global/api-kit": "^4.0.0",
    "@safe-global/protocol-kit": "^5.1.1",
    "@safe-global/types-kit": "^3.0.0",
    "commander": "^12.1.0",
    "conf": "^13.0.1",
    "picocolors": "^1.1.1",
    "viem": "^2.21.8",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "@vitest/ui": "^2.1.8",
    "eslint": "^9.17.0",
    "prettier": "^3.4.2",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
